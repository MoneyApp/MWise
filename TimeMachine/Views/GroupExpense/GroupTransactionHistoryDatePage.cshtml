@model TimeMachine.Models.GroupTransactionHistoryDateViewModel

@using (Html.BeginForm("a", "c", FormMethod.Get))
{
    @Html.AntiForgeryToken()

    <div class="form-group">
        @Html.LabelFor(m => m.StartDate, htmlAttributes: new { @class = "control-label" })
        <div class="input-group date" id="StartDatePicker">
            @Html.TextBoxFor(m => m.StartDate, htmlAttributes: new { @class = "form-control mapp-datefield" })
            <span class="input-group-addon">
                <span class="glyphicon glyphicon-calendar"></span>
            </span>
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(m => m.EndDate, htmlAttributes: new { @class = "control-label " })
        <div class="input-group date" id="EndDatePicker">
            @Html.TextBoxFor(m => m.EndDate, htmlAttributes: new { @class = "form-control mapp-datefield" })
            <span class="input-group-addon">
                <span class="glyphicon glyphicon-calendar"></span>
            </span>
        </div>
    </div>

    <div class="col-md-offset-5">
        <input type="submit" class="btn btn-primary" />
    </div>
}